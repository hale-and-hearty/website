<!-- Make Dropdown nav fire on click instead of hover for mobile/tablets -->
<script>
  $(document).on('click focus', function (e) {
    $link = $('.header li.has-dropdown > a');
    $link.next('ul').hide(); 
    $link.attr('aria-expanded', 'false')
  });
  $('.header li.has-dropdown > a').on('click focus mouseover', function (e) {
    e.stopPropagation();
    e.preventDefault();
    $dropdownEl = $(this).next('ul');
    $dropdownEl.toggle();
    $(this).attr('aria-expanded', $dropdownEl.css('display') === 'block');
  });
</script>

  
